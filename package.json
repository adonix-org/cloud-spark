{
    "name": "@adonix.org/cloud-spark",
    "version": "0.0.181",
    "description": "Ignite your Cloudflare Workers with a type-safe library for rapid development.",
    "license": "Apache-2.0",
    "type": "module",
    "sideEffects": false,
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "author": {
        "name": "Ty Busby",
        "email": "tybusby@adonix.org",
        "url": "https://www.adonix.org"
    },
    "keywords": [
        "cloudflare",
        "workers",
        "typescript",
        "javascript",
        "quickstart",
        "routes",
        "cors"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adonix-org/cloud-spark.git"
    },
    "bugs": {
        "email": "support@adonix.org",
        "url": "https://github.com/adonix-org/cloud-spark/issues"
    },
    "scripts": {
        "types": "wrangler types --config wrangler.jsonc ./types/cloudflare.d.ts",
        "prebuild": "npm run lint && mkdir -p ./types",
        "build": "npm run types && tsup",
        "dev-types": "wrangler types --config wrangler.local.jsonc ./types/cloudflare.d.ts",
        "dev": "npm run dev-types && wrangler dev --config wrangler.local.jsonc",
        "dev-debug": "npm run dev-types && wrangler dev --config wrangler.local.jsonc --log-level debug",
        "test": "npm run build && vitest",
        "coverage": "npm run build && vitest run --coverage",
        "lint": "eslint ./src"
    },
    "devDependencies": {
        "@eslint/js": "^9.38.0",
        "@types/node": "^24.8.1",
        "@typescript-eslint/eslint-plugin": "^8.46.1",
        "@typescript-eslint/parser": "^8.46.1",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.38.0",
        "eslint-plugin-import": "^2.32.0",
        "globals": "^16.4.0",
        "jiti": "^2.6.1",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.1",
        "vitest": "^3.2.4",
        "wrangler": "^4.43.0"
    },
    "dependencies": {
        "cache-control-parser": "^2.0.6",
        "http-status-codes": "^2.3.0",
        "path-to-regexp": "^8.3.0"
    }
}
