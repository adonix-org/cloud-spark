{
    "name": "@adonix.org/cloud-spark",
    "version": "0.0.159",
    "description": "Ignite your Cloudflare Workers with a type-safe library for rapid development.",
    "license": "Apache-2.0",
    "type": "module",
    "sideEffects": false,
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "author": {
        "name": "Ty Busby",
        "email": "tybusby@adonix.org",
        "url": "https://www.adonix.org"
    },
    "keywords": [
        "cloudflare",
        "workers",
        "typescript",
        "javascript",
        "quickstart",
        "routes",
        "cors"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adonix-org/cloud-spark.git"
    },
    "bugs": {
        "email": "support@adonix.org",
        "url": "https://github.com/adonix-org/cloud-spark/issues"
    },
    "scripts": {
        "build": "npm run types && tsup",
        "dev": "tsup --watch",
        "test": "npm run build && vitest",
        "coverage": "vitest run --coverage",
        "types": "wrangler types ./types/wrangler.d.ts --no-include-env",
        "debug": "wrangler types ./types/wrangler.d.ts && wrangler dev --config wrangler.local.jsonc"
    },
    "devDependencies": {
        "@types/node": "^24.4.0",
        "@vitest/coverage-v8": "^3.2.4",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4",
        "wrangler": "^4.40.1"
    },
    "dependencies": {
        "cache-control-parser": "^2.0.6",
        "http-status-codes": "^2.3.0",
        "path-to-regexp": "^8.3.0"
    }
}
